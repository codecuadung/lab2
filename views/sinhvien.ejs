<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h3>Danh sách sinh viên</h3>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Masv</th>
                <th>Tensv</th>
            </tr>
        </thead>
        <tbody>
            <% sinhvien.forEach(function(sv){ %>
            <tr>
                <td><%= sv._id %></td>
                <td><%= sv.masv %></td>
                <td><%= sv.tensv %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
